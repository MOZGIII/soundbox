{
  "version": "0.2.0",
  "configurations": [{
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'soundbox'",
      "cargo": {
        "args": [
          "build",
          "--bin=soundbox",
          "--package=soundbox"
        ],
        "filter": {
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}",
      "env": {
        "RUST_BACKTRACE": "1",
        "MALLOC_CHECK_": "2",
      }
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'soundbox'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=soundbox",
          "--package=soundbox"
        ],
        "filter": {
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}",
      "env": {
        "RUST_BACKTRACE": "1",
        "MALLOC_CHECK_": "2",
      }
    }
  ]
}
